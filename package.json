{
  "name": "boilermanager-boilerplates-template",
  "description": "📁 boilerplates repository template meant to be used alongside boilermanager.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvtiradentes/boilermanager-boilerplates-template.git"
  },
  "scripts": {
    "dev": "npm run update",
    "update": "npm run update:json && npm run update:md && npm run update:projects",
    "update:json": "ts-node ./update/update-json.ts",
    "update:md": "ts-node ./update/update-readme.ts",
    "update:projects": "ts-node ./update/update-projects-readme.ts"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "dyn-markdown": "^1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
